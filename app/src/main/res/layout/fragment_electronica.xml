<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/violeta">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <LinearLayout
            style="@style/edit_carrito"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="20sp">
            <ImageView
                android:id="@+id/imgprodtech"
                android:layout_width="80dp"
                android:layout_height="150dp"
                app:srcCompat="@drawable/electronica"
                android:layout_margin="10sp"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tituloproducto1tech"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/titulo_tech"
                    android:textColor="@color/blanco"
                    android:textSize="25sp"/>
                <TextView
                    android:id="@+id/descriptechAud"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/desaudi"
                    android:textColor="@color/blanco"
                    android:textSize="15sp"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/preciolamp_Products"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/violeta"
                        android:textSize="20sp"
                        android:text="@string/precio_productos"
                        android:layout_marginRight="10sp"/>
                    <TextView
                        android:id="@+id/valorlampara"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/violeta"
                        android:text="@string/precioBalon_Products"/>
                    <Button
                        android:id="@+id/Btnagregar2"
                        style="@style/edit_iniciar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:backgroundTint="@null"
                        android:layout_marginStart="5sp"
                        android:text="@string/text_btnagregar"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>



        <LinearLayout
            style="@style/edit_carrito"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="20sp">

            <ImageView
                android:id="@+id/imgprotech2"
                android:layout_width="80dp"
                android:layout_height="128dp"
                android:layout_margin="10sp"
                app:srcCompat="@drawable/electronica" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tituloOrga"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/orgatitulo"
                    android:textColor="@color/blanco"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/descripropa_products"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/des_orga"
                    android:textColor="@color/blanco"
                    android:textSize="15sp"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/precioorga"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/violeta"
                        android:textSize="20sp"
                        android:text="@string/precio_productos"
                        android:layout_marginRight="10sp"/>
                    <TextView
                        android:id="@+id/valorOrga"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/violeta"
                        android:text="300000"/>
                    <Button
                        android:id="@+id/Btnagregar2"
                        style="@style/edit_iniciar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:backgroundTint="@null"
                        android:layout_marginStart="5sp"
                        android:text="@string/text_btnagregar"/>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/edit_carrito"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="20sp">
            <ImageView
                android:id="@+id/imgport"
                android:layout_width="80dp"
                android:layout_height="150dp"
                app:srcCompat="@drawable/electronica"
                android:layout_margin="10sp"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tituloPort"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tituloPOrt"
                    android:textColor="@color/blanco"
                    android:textSize="30sp"/>
                <TextView
                    android:id="@+id/desporta"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/des_Porta"
                    android:textColor="@color/blanco"
                    android:textSize="15sp"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/precioPort"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/violeta"
                        android:textSize="20sp"
                        android:text="@string/precio_productos" />
                    <TextView
                        android:id="@+id/valorport"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/violeta"
                        android:text="@string/valor_prod"/>
                    <Button
                        android:id="@+id/Btnadd"
                        style="@style/edit_iniciar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:backgroundTint="@null"
                        android:layout_marginStart="5sp"
                        android:text="@string/text_btnagregar"/>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/edit_carrito"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="20sp">
            <ImageView
                android:id="@+id/imgprod5"
                android:layout_width="80dp"
                android:layout_height="150dp"
                app:srcCompat="@drawable/electronica"
                android:layout_margin="10sp"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/titPower"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tit_power"
                    android:textColor="@color/blanco"
                    android:textSize="30sp"/>
                <TextView
                    android:id="@+id/descrielec_products"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/des_power"
                    android:textColor="@color/blanco"
                    android:textSize="15sp"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/preciotech"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/violeta"
                        android:textSize="20sp"
                        android:text="@string/precio_productos"
                        android:layout_marginRight="10sp"/>
                    <TextView
                        android:id="@+id/valortech"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/violeta"
                        android:text="@string/valor_Set"/>

                    <Button
                        android:id="@+id/Btnagregar4"
                        style="@style/edit_iniciar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:backgroundTint="@null"
                        android:layout_marginStart="5sp"
                        android:text="@string/text_btnagregar"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/edit_carrito"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginVertical="20sp">
            <ImageView

                android:id="@+id/imgtech5"
                android:layout_width="80dp"
                android:layout_height="150dp"
                app:srcCompat="@drawable/electronica"
                android:layout_margin="10sp"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tituloAroma"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tit_parlante"
                    android:textColor="@color/blanco"
                    android:textSize="30sp"/>
                <TextView
                    android:id="@+id/desAcc_produc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/des_parlante"
                    android:textColor="@color/blanco"
                    android:textSize="15sp"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/precioparlante"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/violeta"
                        android:textSize="20sp"
                        android:text="@string/precio_productos"
                        android:layout_marginRight="20sp"/>

                    <TextView
                        android:id="@+id/valorparla"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/violeta"
                        android:text="@string/_60000"/>
                    <Button
                        android:id="@+id/Btnagregar5"
                        style="@style/edit_iniciar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:backgroundTint="@null"
                        android:layout_marginStart="10sp"
                        android:text="@string/text_btnagregar"/>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


</ScrollView>